<?xml version="1.0"?>
            <!DOCTYPE hibernate-mapping PUBLIC
                "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


      
<hibernate-mapping>
<class 
    name="Model.BatchFile" 
    table="BCCSC02_OWNER.BATCH_FILE"
>
	<!-- BAF_FILE_SEQ: NUMBER(20) NOT NULL -->
    <id
        name="eventSeqNumber"
        column="BAF_FILE_SEQ_NBR"
    >
        <generator class="assigned" />
    </id>
    <!-- BAF_FILE_NAME: VARCHAR2(50) NULL -->    
    <property
        name="fileName"
        column="BAF_FILE_NM"
    />
    <!-- BAF_FILE_TYPE: VARCHAR2(10) NULL -->
    <property
        name="fileType"
        column="BAF_FILE_TYP_CD"
    />
    <!-- BAF_FILE_DATE: Date NULL -->
    <property
        name="fileDate"
        column="BAF_FILE_DT"
    />
</class>
</hibernate-mapping>
